{
  "name": "whatufind",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "build:android": "cd android && ./gradlew assembleRelease",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "format": "prettier --check ./src/**/*.{ts,tsx}",
    "format:fix": "prettier --write ./src/**/*.{ts,tsx}",
    "rename": "react-native-rename",
    "pod": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.5.0",
    "@shopify/flash-list": "^1.7.2",
    "@shopify/restyle": "^2.4.4",
    "@types/react-native-vector-icons": "^6.4.18",
    "lottie-react-native": "^7.1.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "18.3.1",
    "react-native": "0.76.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.1.0",
    "react-native-reanimated": "^3.16.1",
    "react-native-rename": "^3.2.14",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^3.35.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^15.8.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.2.0",
    "sonner-native": "^0.16.1",
    "zustand": "^5.0.0"
